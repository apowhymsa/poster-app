{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "app/components/*"
      ],
      "@screens/*": [
        "app/screens/*"
      ],
      "@utils/*": [
        "app/utils/*"
      ],
      "@navigation/*": [
        "app/navigation/*"
      ]
    }
  }
}
